{
  "name": "i18next-callback-backend",
  "version": "1.0.0-alpha.1",
  "description": "A \"custom callback\" backend for i18next.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/i18next-callback-backend.js && NODE_ENV=production webpack src/index.js dist/i18next-callback-backend.min.js",
    "lint": "eslint src test examples",
    "test": "NODE_ENV=test mocha",
    "test:watch": "NODE_ENV=test mocha --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd",
    "esdoc": "esdoc -c esdoc.config.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mathieumg/i18next-callback-backend/i18next-callback-backend.git"
  },
  "keywords": [
    "i18n",
    "i18next",
    "i18next-backend"
  ],
  "author": "Mathieu M-Gosselin <mathieumg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mathieumg/i18next-callback-backend/issues"
  },
  "homepage": "https://github.com/mathieumg/i18next-callback-backend",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^1.10",
    "eslint-config-airbnb": "2.1.1",
    "eslint-plugin-react": "^3.1.16",
    "esdoc": "^0.1.3",
    "expect": "^1.13.4",
    "isparta": "^3.0.3",
    "mocha": "^2.3.4",
    "rimraf": "^2.4.4",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "invariant": "^2.2.0"
  }
}
